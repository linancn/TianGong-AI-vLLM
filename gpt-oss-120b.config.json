{
  "apps": [
    {
      "name": "vllm-gpt120b",
      "script": "/bin/bash",
      "args": [
        "-lc",
        "source .venv/bin/activate && VLLM_USE_MODELSCOPE=True python -m vllm.entrypoints.openai.api_server --model openai-mirror/gpt-oss-120b"
      ],
      "interpreter": "none",
      "env": {
        "CUDA_VISIBLE_DEVICES": "2"
      },
      "autorestart": true,
      "watch": false,
      "error_file": "./logs/vllm-error.log",
      "out_file": "./logs/vllm-out.log",
      "merge_logs": true,
      "time": true
    }
  ]
}